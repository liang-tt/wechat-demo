/* pages/around/around.wxss */
.inputContainer {
  margin: 20rpx 10rpx;
}
.searchInput {
  background:#eee;
  text-align:center;
  height:70rpx;
  line-height:70rpx;
  border-radius:10rpx;
  font-weight:normal;
  font-size:28rpx;
}
.searchPlace {
  font-size: 28rpx;
}
.serachContainer {
  height:300rpx;
  z-index:1000;
  position:absolute;
  background: #eee;
}


.contentLetter {
  display: flex;
  flex-direction:row-reverse;
}
.letterContainer {
  display: flex;
  flex-direction: column;
  width: 50rpx;
  font-size: 28rpx;
  text-align:center;
}

.cityList {
  flex-grow: 1;
  color: #666;
  padding:10rpx;
}
.letter {
  padding:8rpx 0;
  font-size:24rpx;
}
.cityLetter {
  background:#eee;
  line-height:60rpx;
  padding-left:20rpx;
}
.cityTitle {
  height:60rpx;
  line-height:60rpx;
  border-bottom:1rpx solid #eee;
  font-size:30rpx;
  padding:10rpx 20rpx;
}
.returnTop {
  color:#2a579a;
  font-size:28rpx;
}